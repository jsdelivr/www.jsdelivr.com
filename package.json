	"scripts": {
		"build": "gulp build",
		"heroku-postbuild": "echo \"$SOURCE_VERSION\" > sha.txt",
		"lint": "npm run lint:css && npm run lint:html && npm run lint:js",
		"lint:fix": "npm run lint:css:fix && npm run lint:html:fix && npm run lint:js:fix",
		"lint:css": "stylelint --config .stylelintrc \"./src/**/*.{css,less}\"",
		"lint:css:fix": "stylelint --fix --config .stylelintrc \"./src/**/*.{css,less}\"",
		"lint:html": "eslint --no-eslintrc --config .eslintrc-html --cache-location .eslintcache-html --cache --ext .html .",
		"lint:html:fix": "eslint --no-eslintrc --config .eslintrc-html --cache-location .eslintcache-html --cache --fix --ext .html .",
		"lint:js": "eslint --cache --ext .html,.js,.json .",
		"lint:js:fix": "eslint --cache --fix --ext .html,.js,.json .",
		"mocha": "mocha --exit test/tests.js",
		"prepare": "husky install",
		"start": "cross-env NODE_ENV=development gulp",
		"test": "npm run lint && npm run mocha"
	},
	"lint-staged": {
		"*.{css,less}": "stylelint --fix",
		"*.{html,js}": "eslint --cache --fix",
		"*.html": "eslint --no-eslintrc --config .eslintrc-html --cache-location .eslintcache-html --cache --fix --ext .html"
